{
  "name": "@kareemjeiroudi/reactive-models",
  "version": "0.1.5",
  "description": "A parent-child reactive contract with ergonomic two-way binding. This contract pattern is more ergonomic than React's native reactive API.",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/dts/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun run ./build.ts",
    "build:watch": "bun run --watch ./build.ts --watch",
    "build:production": "bun run ./build.ts --production",
    "compile": "tsc",
    "prepublish": "bun build",
    "lint": "biome lint",
    "lint:write": "biome lint --write",
    "format": "biome format",
    "format:write": "biome format --write"
  },
  "author": "Kareem Jeiroudi",
  "license": "MIT",
  "contributors": [{
    "name": "Kareem Jeiroudi",
    "email": "kareemjeiroudi@gmail.com"
  }],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@biomejs/biome": "^2.2.7",
    "@types/bun": "^1.3.0",
    "chokidar": "^4.0.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "react-hotkeys-hook": "^5.2.1"
  }
}